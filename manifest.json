{
  "action": {
    "default_title": "Emulate Dislexical disorder",
    "default_popup": "src/popup/index.html"
  },
  "background": {
    "service_worker": "src/service-worker.js",
    "type": "module"
  },
  "default_locale": "en",
  "description": "__MSG_manifest_app_description__",
  "host_permissions": [
    "*://*/?*"
  ],
  "icons": {
    "16": "images/logo-default16.png",
    "32": "images/logo-default32.png",
    "64": "images/logo-default64.png",
    "128": "images/logo-default128.png"
  },
  "manifest_version": 3,
  "minimum_chrome_version": "92",
  "name": "__MSG_manifest_app_name__",
  "options_ui": {
    "page": "settings/settings.html",
    "open_in_tab": false
  },
  "permissions": [
    "activeTab",
    "declarativeContent",
    "scripting",
    "storage",
    "tabs"
  ],
  "version": "0.0.2",
  "web_accessible_resources": [{
    "resources": ["src/dislexic.js"],
    "matches": [
      "http://*/*",
      "https://*/*"
    ]
  }]
}
